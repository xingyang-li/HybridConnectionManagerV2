{
  "executable": "HybridConnectionManagerGUI",
  "splashscreen": {
    "imageFile": ""
  },
  "name": "HybridConnectionManagerGUI",
  "author": "",
  "singleInstance": false,
  "environment": "Production",
  "electronPort":  4999,
  "build": {
    "appId": "com.HybridConnectionManagerGUI.app",
    "productName": "HybridConnectionManagerGUI",
    "copyright": "Copyright Â© 2020",
    "buildVersion": "1.0.0",
    "compression": "maximum",
    "directories": {
      "output": "../../../bin/Desktop"
    },
    "win": {
      "icon": "bin/wwwroot/assets/icon.ico"
    },
    "mac": {
      "icon": "bin/wwwroot/assets/icon.icns"
    },
    "linux": {
      "icon": "bin/wwwroot/assets/icon.png"
    },
    "extraResources": [
      {
        "from": "./bin",
        "to": "bin",
        "filter": [ "**/*" ]
      }
    ],
    "files": [
      {
        "from": "./ElectronHostHook/node_modules",
        "to": "ElectronHostHook/node_modules",
        "filter": [ "**/*" ]
      },
      "**/*"
    ]
  }
}